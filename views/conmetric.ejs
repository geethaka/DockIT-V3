      var name = document.getElementById("total").innerHTML;
      var ip = document.getElementById("ip").innerHTML;
      data.name = name ;
      data.ip = ip ; 
      

     $.ajax({
    type: 'POST',
    data: JSON.stringify(data),
    contentType: 'application/json',
            url: 'http://localhost:3000/top',
            success: function(data) {
                console.log('success');
                //console.log(JSON.stringify(data));

                //var l = data.json;
               // var val = data.data;
               // $("#status").val(val[0]);
                 //$('#messages').append($('<li>').text(data.name));
               
                 document.getElementById("work").innerHTML =  data.cpu;
                 document.getElementById("used").innerHTML =  data.mem+' MB';
                 document.getElementById("total1").innerHTML =  data.meml+' GB';
                  document.getElementById("net1").innerHTML =  data.net;
                   document.getElementById("net2").innerHTML =  data.net2;
                 var len = data.cpu.length;
                 console.log(data.cpu.substring(0,len-2));
                 g.refresh(data.cpu.substring(0,len-1));
                 g1.refresh(data.memp.substring(0,len-1));
                 console.log(data.name);
             


            }



  });


}

setInterval(doSomething, 5000); // Time in milliseconds


